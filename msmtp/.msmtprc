defaults
auth            on
tls             on
tls_trust_file  /usr/local/etc/openssl/cert.pem
logfile         ~/.msmtp.log

account         gpanders
host            mail.gandi.net
port            587
from            greg@gpanders.com
user            greg@gpanders.com
passwordeval    pass mail/greg@gpanders.com; echo

account         gmail
host            smtp.gmail.com
port            587
from            greg.p.anders@gmail.com
user            greg.p.anders
passwordeval    pass google/apps/msmtp; echo

account default: gpanders

# vim: ft=msmtp
