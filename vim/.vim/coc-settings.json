{
  "coc.preferences.autoTrigger": "none",
  "languageserver": {
    "cquery": {
      "command": "cquery",
      "args": ["--log-file=/tmp/cquery/cquery.log"],
      "revealOutputChannelOn": "never",
      "filetypes": ["c", "cpp"],
      "rootPatterns": ["compile_commands.json", ".git/"],
      "initializationOptions": {
        "cacheDirectory": "/tmp/cquery/cache"
      }
    }
  }
}
